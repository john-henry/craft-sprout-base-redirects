{
  "name": "barrelstrength/sprout-base-redirects",
  "description": "Common Redirects libraries for Sprout Plugins for Craft CMS",
  "version": "4.0.0",
  "type": "yii-module",
  "license": "proprietary",
  "authors": [
    {
      "name": "Barrel Strength",
      "homepage": "https://www.barrelstrengthdesign.com/"
    }
  ],
  "support": {
    "email": "sprout@barrelstrengthdesign.com",
    "issues": "https://github.com/barrelstrength/sprout-base-redirects/issues?state=open",
    "source": "https://github.com/barrelstrength/sprout-base-redirects",
    "docs": "https://sprout.barrelstrengthdesign.com/craft-plugins/seo/docs",
    "rss": "https://github.com/barrelstrength/sprout-base-redirects/commits/v2.atom"
  },
  "require": {
    "craftcms/commerce": "^4.0.0",
    "barrelstrength/sprout-base": "^6.0.0"
  },
  "autoload": {
    "psr-4": {
      "barrelstrength\\sproutbaseredirects\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require-dev": {
    "craftcms/rector": "dev-main"
  }
}
